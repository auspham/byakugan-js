{
    "name": "byakugan-js",
    "version": "0.0.2",
    "description": "Path finding algorithm for sensory type ninjas.",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "dev": "watch 'npm run build' src",
        "build": "tsc && browserify build/index.js -s Byakugan > dist/byakugan.js && uglifyjs dist/byakugan.js -o dist/byakugan.min.js",
        "test": "jest",
        "coverage": "jest --coverage",
        "deploy": "gh-pages -d examples"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rockmanvnx6/byakugan.git"
    },
    "keywords": [
        "Path",
