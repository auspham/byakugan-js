        "Array",
        "Astar",
        "ninja"
    ],
    "author": "rockmanvnx6",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^26.0.4",
        "browserify": "^16.5.1",
        "gh-pages": "^3.1.0",
        "jest": "^26.1.0",
        "ts-jest": "^26.1.1",
        "typescript": "^3.9.6",
        "uglify-js": "^3.10.0",
        "watch": "^1.0.2"
    },
    "dependencies": {}
}
